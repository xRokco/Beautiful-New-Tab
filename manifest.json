{
	"name": "Beautiful New Tab",
	"version": "1.1",
	"minimum_chrome_version": "37",
	"manifest_version": 2,
	"description": "A beautiful new tab page",
	"icons": {
		"128": "icon_128.png",
		 "48": "icon_48.png",
		 "16": "icon_16.png"
	},
	"permissions": [
		"geolocation",
		"storage",
		"unlimitedStorage"
	],
	"background": {
    	"scripts": ["jquery.min.js","config.js","background.js"]
  	},
	"chrome_url_overrides":
	{
		"newtab": "index.html"
	},
	"options_page": "options.html",
	"web_accessible_resources": [
		"index.html"
	]
}